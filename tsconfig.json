{
	"compilerOptions": {
		"declaration": true,
		"forceConsistentCasingInFileNames": true,
		"noEmitOnError": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
	//	"strictNullChecks": true,
	"suppressImplicitAnyIndexErrors": true,
		"target": "es5"
	},
	"exclude": [
		"node_modules",
		"tests"
	]
}
